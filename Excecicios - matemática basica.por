programa {

  inteiro ano, atualano = 2023 
  cadeia nome 

  funcao inicio() {

    escreva("Olá, informe seu nome: ")
    leia (nome)
    limpa ()
    escreva("Legal " , nome , ", Informe agora, qual ano que você nasceu: ")
    leia (ano)
    limpa ()
    escreva("Bom " , nome , ", atualmente você tem aproximadamente " , (atualano-ano) , " anos. " )
    
    
  }
}
