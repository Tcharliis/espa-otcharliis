programa {

  inteiro vl1 , vl2
  funcao inicio() {

    escreva("Matemática Básica")
    escreva("\nGentileza me informe número inteiro: ")
    leia (vl1)
    limpa ()
    escreva("Informe segundo número interio: ")
    leia (vl2)
    limpa ()
    escreva("Resuldado dos dois número é: " , (vl1+vl2) )
    
    
  }
}
